{
  "name": "midnight-rose-theme",
  "displayName": "Midnight Rose Theme",
  "description": "A sophisticated dark theme with elegant rose accents for a calm coding experience.",
  "version": "1.1.1",
  "publisher": "nathan-dev",
  "icon": "logo.png",
  "main": "./extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "engines": {
    "vscode": "^1.70.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nathan/rose-dark-theme"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Rose Dark Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/rose-dark-theme.json"
      }
    ],
    "iconThemes": [
      {
        "id": "rose-dark-icons",
        "label": "Rose Dark Icons",
        "path": "./rose-dark-icon-theme.json"
      }
    ]
  }
}
